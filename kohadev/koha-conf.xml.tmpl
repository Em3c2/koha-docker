            <database>authorities</database>
          </serverInfo>
        </explain>
      </xi:fallback>
  </xi:include> -->
</server>
<serverinfo id="authorityserver">
    <ccl2rpn>/etc/koha/zebradb/ccl.properties</ccl2rpn>
    <user>$KOHA_ZEBRAUSER</user>
    <password>$KOHA_ZEBRAPASS</password>
</serverinfo>

